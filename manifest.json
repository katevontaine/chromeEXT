{
  "manifest_version": 2,
  "name": "NEW YORK TIMES MOST POPULAR EXTENSION",
  "description": "Shows msot shared and emailed posts.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Click here!"

  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://api.nytimes.com/"
  ]




}
